@prefix eurovoc: <http://eurovoc.europa.eu/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rb: <https://w3id.org/riverbench/schema/metadata#> .
@prefix rbdoc: <https://w3id.org/riverbench/schema/documentation#> .
